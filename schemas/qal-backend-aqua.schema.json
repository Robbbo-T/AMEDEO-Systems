{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "required": ["id","language","version","backend","features"],
  "properties": {
    "id": {"type":"string"},
    "language": {"type":"string","const":"QAL"},
    "version": {"type":"string"},
    "backend": {"type":"string","const":"aqua"},
    "features": {"type":"array", "items":{"type":"string"}, "minItems":1}
  }
}
