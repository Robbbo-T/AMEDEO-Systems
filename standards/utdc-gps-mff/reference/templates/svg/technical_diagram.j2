<svg xmlns="http://www.w3.org/2000/svg" width="800" height="600" viewBox="0 0 800 600">
  <metadata>
    <!-- MFF metadata embedding -->
    <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
             xmlns:utdc="http://amedeo-systems.com/utdc-gps-mff/1.0">
      <rdf:Description rdf:about="">
        <utdc:title>${TITLE}</utdc:title>
        <utdc:description>${DESCRIPTION}</utdc:description>
        <utdc:author>${AUTHOR}</utdc:author>
        <utdc:generated>${DATE_UTC}</utdc:generated>
        <utdc:version>${VERSION}</utdc:version>
        <utdc:standard>UTDC-GPS-MFF v1.0</utdc:standard>
      </rdf:Description>
    </rdf:RDF>
  </metadata>
  
  <!-- Background -->
  <rect width="800" height="600" fill="#f8f9fa" stroke="#dee2e6" stroke-width="2"/>
  
  <!-- Header -->
  <rect x="20" y="20" width="760" height="80" fill="#007bff" rx="5"/>
  <text x="400" y="50" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="18" font-weight="bold">
    UTDC-GPS-MFF Technical Diagram
  </text>
  <text x="400" y="75" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="14">
    ${TITLE}
  </text>
  
  <!-- Main content area -->
  <g transform="translate(50, 150)">
    <!-- MFF Process Flow -->
    <rect x="0" y="0" width="120" height="60" fill="#28a745" rx="5"/>
    <text x="60" y="30" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="12" font-weight="bold">
      Template
    </text>
    <text x="60" y="45" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="12">
      Processing
    </text>
    
    <!-- Arrow -->
    <path d="M 130 30 L 170 30" stroke="#6c757d" stroke-width="2" marker-end="url(#arrowhead)"/>
    
    <rect x="180" y="0" width="120" height="60" fill="#ffc107" rx="5"/>
    <text x="240" y="30" text-anchor="middle" fill="black" font-family="Arial, sans-serif" font-size="12" font-weight="bold">
      Content
    </text>
    <text x="240" y="45" text-anchor="middle" fill="black" font-family="Arial, sans-serif" font-size="12">
      Generation
    </text>
    
    <!-- Arrow -->
    <path d="M 310 30 L 350 30" stroke="#6c757d" stroke-width="2" marker-end="url(#arrowhead)"/>
    
    <rect x="360" y="0" width="120" height="60" fill="#dc3545" rx="5"/>
    <text x="420" y="30" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="12" font-weight="bold">
      Validation
    </text>
    <text x="420" y="45" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="12">
      &amp; Signing
    </text>
    
    <!-- Arrow -->
    <path d="M 490 30 L 530 30" stroke="#6c757d" stroke-width="2" marker-end="url(#arrowhead)"/>
    
    <rect x="540" y="0" width="120" height="60" fill="#6610f2" rx="5"/>
    <text x="600" y="30" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="12" font-weight="bold">
      MFF
    </text>
    <text x="600" y="45" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="12">
      Artifact
    </text>
  </g>
  
  <!-- Compliance indicators -->
  <g transform="translate(50, 280)">
    <text x="0" y="20" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#495057">
      Compliance Standards:
    </text>
    
    <circle cx="20" cy="50" r="8" fill="#28a745"/>
    <text x="35" y="55" font-family="Arial, sans-serif" font-size="12" fill="#495057">
      UTCS-MI v5.0+
    </text>
    
    <circle cx="150" cy="50" r="8" fill="#28a745"/>
    <text x="165" y="55" font-family="Arial, sans-serif" font-size="12" fill="#495057">
      S1000D v5.0
    </text>
    
    <circle cx="280" cy="50" r="8" fill="#28a745"/>
    <text x="295" y="55" font-family="Arial, sans-serif" font-size="12" fill="#495057">
      DO-178C
    </text>
    
    <circle cx="400" cy="50" r="8" fill="#28a745"/>
    <text x="415" y="55" font-family="Arial, sans-serif" font-size="12" fill="#495057">
      CS-25
    </text>
  </g>
  
  <!-- Metadata section -->
  <g transform="translate(50, 380)">
    <rect x="0" y="0" width="700" height="150" fill="#e9ecef" stroke="#adb5bd" stroke-width="1" rx="5"/>
    <text x="10" y="20" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#495057">
      Document Metadata
    </text>
    
    <text x="10" y="45" font-family="Arial, sans-serif" font-size="12" fill="#6c757d">
      Description: ${DESCRIPTION}
    </text>
    <text x="10" y="65" font-family="Arial, sans-serif" font-size="12" fill="#6c757d">
      Author: ${AUTHOR}
    </text>
    <text x="10" y="85" font-family="Arial, sans-serif" font-size="12" fill="#6c757d">
      Generated: ${DATE_UTC}
    </text>
    <text x="10" y="105" font-family="Arial, sans-serif" font-size="12" fill="#6c757d">
      Version: ${VERSION}
    </text>
    <text x="10" y="125" font-family="Arial, sans-serif" font-size="12" fill="#6c757d">
      Standard: UTDC-GPS-MFF v1.0
    </text>
  </g>
  
  <!-- Arrow marker definition -->
  <defs>
    <marker id="arrowhead" markerWidth="10" markerHeight="7" 
            refX="9" refY="3.5" orient="auto">
      <polygon points="0 0, 10 3.5, 0 7" fill="#6c757d"/>
    </marker>
  </defs>
</svg>